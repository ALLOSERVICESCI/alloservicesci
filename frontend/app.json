{
  "expo": {
    "name": "All√¥ Services CI",
    "slug": "allo-services-ci",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icons/icon.png",
    "scheme": "allo-services-ci",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "notification": {
      "color": "#0A7C3A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ci.alloservices.mobile"
    },
    "android": {
      "package": "ci.alloservices.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/icons/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "permissions": ["POST_NOTIFICATIONS", "SCHEDULE_EXACT_ALARM"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/icons/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/icons/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ],
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}